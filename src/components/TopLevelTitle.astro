---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<h1>{title}</h1>

<style>
	@import url('/src/styles/config/_mixins.css');

	h1 {
		@mixin body-font-bold;
		font-size: 2.4rem;
		hyphens: auto;
		line-height: 1;
		margin-block-end: 1.5rem;
		text-transform: uppercase;
	}
</style>
